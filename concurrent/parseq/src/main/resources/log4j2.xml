<?xml version="1.0" encoding="UTF-8"?>
<Configuration status="WARN">
    <Appenders>
        <Console name="Console" target="SYSTEM_OUT">
            <PatternLayout pattern="%d{yyyy-MM-dd HH:mm:ss.SSS} %X{transaction.owner} [%t] [%-5p]  %c {%F:%L} - %m%n"/>
        </Console>
    </Appenders>
    <Loggers>
        <!--<Logger name="com" level="info"/>-->

        <Root level="trace">
            <AppenderRef ref="Console"/>
        </Root>
    </Loggers>
</Configuration>